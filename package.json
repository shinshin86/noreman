{
  "name": "noreman",
  "version": "0.0.1",
  "description": "Manage Procfile-based applications (foreman and goreman clone written in Node.js)",
  "main": "dist/noreman.js",
  "bin": {
    "noreman": "dist/noreman.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "node -r @swc-node/register src/noreman.ts",
    "prepublishOnly": "npm run build",
    "fmt": "deno fmt",
    "test": "jest"
  },
  "keywords": [
    "devops",
    "dev ops",
    "procfile",
    "process manager",
    "goreman",
    "foreman"
  ],
  "author": "shinshin86 <shinshin86npm@gmail.com> (https://github.com/shinshin86)",
  "license": "MIT",
  "devDependencies": {
    "@swc-node/jest": "^1.5.2",
    "@swc-node/register": "^1.5.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.34",
    "jest": "^28.1.0",
    "typescript": "^4.6.4"
  }
}
